               <s:assert xmlns="http://www.tei-c.org/ns/1.0"
                         xmlns:rng="http://relaxng.org/ns/structure/1.0"
                         xmlns:s="http://purl.oclc.org/dsdl/schematron"
                         test="matches(@from, '[0-9]+[a-z]{1,3}')">L'attribut @from à pour
                           valeur un nombre suivi de une à trois lettres</s:assert>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="oddbyexample-locus-toFol-constraint-assert-13">
            <rule context="tei:locus">
               <s:assert xmlns="http://www.tei-c.org/ns/1.0"
                         xmlns:rng="http://relaxng.org/ns/structure/1.0"
                         xmlns:s="http://purl.oclc.org/dsdl/schematron"
                         test="matches(@to, '[0-9]+[a-z]{1,3}')">L'attribut @to à pour
                           valeur un nombre suivi de une à trois lettres</s:assert>
            </rule>
